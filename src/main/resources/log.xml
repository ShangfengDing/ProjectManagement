<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="log" namespace="/log" extends="default">

        <action name="findLogByPid"  class="com.free4lab.freeRT.action.LogAction">
            <result name="success" type="json"></result>
        </action>

        <action name="findLogByUid"  method="findLogByUid" class="com.free4lab.freeRT.action.LogAction">
            <result name="success" type="json"></result>
        </action>

        <action name="deleteLog"  method="deleteLog" class="com.free4lab.freeRT.action.LogAction">
            <result name="success" type="json"></result>
        </action>

    </package>
</struts>